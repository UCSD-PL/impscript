extern val _plus : (num, num) -> num;

type numlist = mu 'x. {hd: num, tl: ?('x)};

var f; f = function [L] (xs: *L) / (*L: numlist) -> *L / () {
  [unfold(numlist, xs);]
  xs.hd = _plus(1, xs.hd);
  [fold(numlist, xs);]
  return xs;
};

var obj; obj = {hd = 0, tl = null};

[fold(numlist, obj);]
f(obj);

(** ERROR: the function type threw away the location **)
[unfold(numlist, obj);]

_plus(1, obj.hd);
